{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:react/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "project": [
      "tsconfig.eslint.json"
    ]
  },
  "plugins": [
    "import",
    "@typescript-eslint",
    "jsx-a11y",
    "react"
  ],
  "rules": {},
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.common.js"
      }
    },
    "react": {
      "version": "detect"
    }
  }
}
