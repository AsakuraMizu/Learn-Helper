{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended",
        "tslint-config-airbnb",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "ter-arrow-parens": false,
        "align": false,
        "ordered-imports": false,
        "import-name": false,
        "variable-name": false,
        "jsx-no-multiline-js": false,
        "jsx-wrap-multiline": false,
        "jsx-no-lambda": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "src/css"
        ]
    }
}